{
  "name": "reputon-demo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test:ui": "npx playwright test --project=google-reviews",
    "test:component": "npx playwright test --project=google-reviews --grep @component",
    "test:integration": "npx playwright test --project=google-reviews --grep @integration",
    "html-report-open": "npx playwright show-report",
    "allure-report": "allure generate allure-results -o allure-report --clean",
    "allure-report-open": "npm run allure-report && allure open allure-report",
    "ui-mode": "npx playwright test --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "npm run prettier && npm run lint",
    "format:fix": "npm run prettier:fix && npm run lint:fix",
    "prepare": "husky",
    "notify": "ts-node src/scripts/notification.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@playwright/test": "^1.55.0",
    "@types/dotenv": "^6.1.1",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.0.13",
    "@types/node-telegram-bot-api": "^0.64.9",
    "@types/rimraf": "^3.0.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.36.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^3.3.1",
    "dotenv": "^17.2.0",
    "fs-extra": "^11.3.0",
    "lodash": "^4.17.21",
    "node-telegram-bot-api": "^0.66.0",
    "playwright-qase-reporter": "^2.1.4",
    "qaseio": "^2.4.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2"
  }
}
